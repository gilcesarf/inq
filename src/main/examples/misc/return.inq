// The Inq return statement.
// An exceptional way to leave a function

function somefunc(boolean p1)
{
  if (p1)
    return (-99);

  99;
}


writeln($catalog.system.out, "normal return : " + call somefunc());

writeln($catalog.system.out, "explicit return : " + call somefunc(p1=true));
